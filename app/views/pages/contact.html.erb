<div class="container mt-3">
  <h1>Contact</h1>
  <form action="/contact" method="get" class="my-2">
    <input type="text" name="member" class="form-control" value="<%= params[:member] %>">
    <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
    <%= hidden_field_tag :letters, "ASDASDA" %>
    <input type="submit" class="btn btn-primary mt-2">
  </form>
  <p>You searched for: <%= params[:member] %></p>
  <ul>
    <% @members.each do |member| %>
      <li><%= member %></li>
    <% end %>
  </ul>
</div>
